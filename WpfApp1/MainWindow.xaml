<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1.Views"
        mc:Ignorable="d"
        Title="Warehouse client" Height="550" Width="850" >
    <DockPanel x:Name="container" VerticalAlignment="Center" HorizontalAlignment="Center">
        <local:Login Visibility="Visible" x:Name="LoginControl"/>
        <local:Supplier Visibility="Collapsed" x:Name="SupplierControl"/>
        <local:Register Visibility="Collapsed" x:Name="RegisterControl"/>
    </DockPanel>
</Window>
